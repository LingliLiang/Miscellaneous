			<Button name="btn_cancel" text="取消" height="30" width="80" visible="fasle" normalimage="file='common\\btn_normal.png' corner='4,4,4,4' " hotimage="file='common\\btn_hot.png' corner='4,4,4,4' " pushedimage="file='common\\btn_pushed.png' corner='4,4,4,4' " />
		</HorizontalLayout>
	</VerticalLayout>
